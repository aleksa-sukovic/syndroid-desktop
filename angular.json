{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "angular-electron": {
            "root": "",
            "sourceRoot": "src/renderer",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist",
                        "index": "src/renderer/index.html",
                        "main": "src/renderer/main.ts",
                        "tsConfig": "src/renderer/tsconfig.app.json",
                        "polyfills": "src/renderer/polyfills.ts",
                        "assets": [
                            "src/renderer/assets",
                            "src/renderer/favicon.ico",
                            "src/renderer/favicon.png",
                            "src/renderer/favicon.icns",
                            "src/renderer/favicon.256x256.png",
                            "src/renderer/favicon.512x512.png"
                        ],
                        "styles": [
                            "src/renderer/styles.scss",
                            "node_modules/typeface-roboto/index.css",
                            "node_modules/@fortawesome/fontawesome-free/css/all.css"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "dev": {
                            "optimization": false,
                            "outputHashing": "all",
                            "sourceMap": true,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": false,
                            "fileReplacements": [
                                {
                                    "replace": "src/renderer/environments/environment.ts",
                                    "with": "src/renderer/environments/environment.dev.ts"
                                }
                            ]
                        },
                        "production": {
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/renderer/environments/environment.ts",
                                    "with": "src/renderer/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "angular-electron:build"
                    },
                    "configurations": {
                        "dev": {
                            "browserTarget": "angular-electron:build:dev"
                        },
                        "production": {
                            "browserTarget": "angular-electron:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "angular-electron:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/renderer/test.ts",
                        "polyfills": "src/renderer/polyfills-test.ts",
                        "tsConfig": "src/renderer/tsconfig.spec.json",
                        "karmaConfig": "src/renderer/karma.conf.js",
                        "scripts": [],
                        "styles": [
                            "src/renderer/styles.scss"
                        ],
                        "assets": [
                            "src/renderer/assets",
                            "src/renderer/favicon.ico",
                            "src/renderer/favicon.png",
                            "src/renderer/favicon.icns",
                            "src/renderer/favicon.256x256.png",
                            "src/renderer/favicon.512x512.png"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/renderer/tsconfig.app.json",
                            "src/renderer/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "angular-electron-e2e": {
            "root": "e2e",
            "projectType": "application",
            "architect": {
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "e2e/tsconfig.e2e.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },
    "defaultProject": "angular-electron",
    "schematics": {
        "@schematics/angular:component": {
            "prefix": "app",
            "styleext": "scss"
        },
        "@schematics/angular:directive": {
            "prefix": "app"
        }
    }
}
